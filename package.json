{
  "name": "quadbin-js",
  "description": "Utility functions for working with Quadbins",
  "license": "MIT",
  "private": true,
  "keywords": ["quadbin", "webgl", "visualization"],
  "repository": {
    "type": "git",
    "url": "https://github.com/CartoDB/quadbin-js.git"
  },
  "scripts": {
    "clean": "ocular-clean",
    "build": "ocular-clean && ocular-build && npm run tsbuild",
    "version": "ocular-build core",
    "lint": "ocular-lint",
    "cover": "ocular-test cover",
    "publish": "ocular-publish",
    "test": "ocular-test",
    "test-fast": "ocular-test fast",
    "test-node": "ocular-test node",
    "test-browser": "ocular-test browser",
    "tsclean": "find . -name tsconfig.tsbuildinfo -exec rm {} \\;",
    "tsbuild": "tsc -b tsconfig.build.json && node scripts/typed-entry.js",
    "bench": "ocular-test bench",
    "bench-browser": "ocular-test bench-browser"
  },
  "browser": {
    "jsdom": false
  },
  "devDependencies": {
    "@babel/register": "^7.13.0",
    "babel-loader": "^8.0.0",
    "babel-preset-minify": "^0.5.0",
    "ocular-dev-tools": "1.0.0-alpha.7",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {}
}
